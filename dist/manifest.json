{
  "manifest_version": 3,
  "name": "Grind Hard - LeetCode & System Design Daily",
  "version": "1.0.0",
  "description": "Daily LeetCode problems and System Design case studies to help you grind consistently",
  "action": {
    "default_popup": "index.html",
    "default_title": "Grind Hard",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
